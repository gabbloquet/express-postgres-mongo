{
  "name": "express-postgres-mongo",
  "version": "1.0.0",
  "description": "NodeJS Server using express which consume mongo & postgres",
  "main": "index.js",
  "scripts": {
    "build": "npx tsc --build --verbose",
    "serve": "nodemon -q dist/index.js",
    "watch": "npx tsc --watch",
    "dev": "concurrently \"npm run watch\" \"npm run serve\""
  },
  "keywords": [
    "server",
    "node",
    "express",
    "postgres",
    "mongo"
  ],
  "author": "BLOQUET Gabin",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.23",
    "@types/sequelize": "^4.28.11",
    "concurrently": "^7.1.0",
    "nodemon": "^2.0.15",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "body-parser": "^1.19.2",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "mongodb": "^4.4.1",
    "mongoose": "^6.2.9",
    "pg": "^8.7.3",
    "sequelize": "^6.17.0"
  }
}
